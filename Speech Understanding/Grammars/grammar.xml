	<!--
	
	Determiners:
		Determines can be:
			Article:		a, an, the
			Possessive:		my, your, his, her, its, our, their
			Demonstrative:	this, that
			Quantifier:		some, many, a lot of, both, all, enough.
	
	//-->

	<rule id="determiner">
		<one-of>
			<item><ruleref uri="#determinerArticle" /></item>
			<item><ruleref uri="#determinerPossesive" /></item>
			<item><ruleref uri="#determinerDemonstrative" /></item>
			<item><ruleref uri="#determinerQuantifier" /></item>
		</one-of>
	</rule>

	<rule id="determinerNQ">
		<one-of>
			<item><ruleref uri="#determinerArticle" /></item>
			<item><ruleref uri="#determinerPossesive" /></item>
			<item><ruleref uri="#determinerDemonstrative" /></item>
		</one-of>
	</rule>
	
	<rule id="determinerArticle">
		<one-of>
			<item>a</item>
			<item>an</item>
			<item>the</item>
		</one-of>
	</rule>
	
	<rule id="determinerPossesive">
		<one-of>
			<item>my</item>
			<item>your</item>
			<!--
			<item>his</item>
			<item>her</item>
			<item>its</item>
			<item>our</item>
			<item>their</item>
			//-->
		</one-of>
	</rule>
	
	<rule id="determinerDemonstrative">
		<one-of>
			<item>this</item>
			<item>that</item>
			<!--
			<item>these</item>
			<item>those</item>
			//-->
		</one-of>
	</rule>

	<rule id="determinerQuantifier">
		<one-of>
			<item>another</item>
			<item>any</item>
			<item>few</item>
			<item>little</item>
			<item>little bit</item>
			<item>lot</item>
			<item>some</item>
			<item><ruleref uri="#number"/></item>
		</one-of>
		<item repeat="0-1">of</item>
	</rule>
	
	<rule id="and_or">
		<one-of>
			<item>and</item>
			<item>or</item>
		</one-of>
	</rule>
	
