<?xml version="1.0" encoding="utf-8" ?>

<!-- the default grammar language is US English -->
<grammar xmlns="http://www.w3.org/2001/06/grammar"
	tag-format="semantics/1.0"
	mode="voice"
	xml:lang="en-GB"
	version="1.0"
	root="sentence">
	
	<rule id="sentence">
		<ruleref uri="#polite" />
		<one-of>
			<item><ruleref uri="#simpleCommand" /></item>
			<item><ruleref uri="#complexCommand" /></item>
			<!--
			<item>
				<item weight="0.01" repeat="1-3">
					<ruleref uri="#stripped" /><item repeat="0-1">and</item>
				</item>
			</item>
			//-->
		</one-of>
		<item repeat="0-1">please</item>
		<!-- <ruleref uri="#garbage" /> //-->
	</rule>
	
	<rule id="sentenceAlt" scope="public">
		<ruleref uri="#polite" />
		<item repeat="1-3">
			<ruleref uri="#stripped" /><item repeat="0-1">and</item>
		</item>
		<item repeat="0-1">please</item>
	</rule>
	
	<rule id="complexCommand">
		<item><ruleref uri="#simpleCommand" /></item>
		<item repeat="1-3"><ruleref uri="#simpleCommand" /></item>
		<item>and <ruleref uri="#simpleCommand" /></item>
	</rule>
	
	<rule id="simpleCommand">
		<one-of>
			<!-- BRINGING //-->
			<item><ruleref uri="#commandBring" /></item>
			<!-- PLACING //-->
			<item><ruleref uri="#commandPlace" /></item>
			<!-- TAKING //-->
			<item><ruleref uri="#commandTake" /></item>
			
			<!-- SEARCHING //-->
			<item><ruleref uri="#commandSearch" /></item>
			<!-- MOTION //-->
			<item><ruleref uri="#commandFollow" /></item>
			<item><ruleref uri="#commandNavigate" /></item>
			<!-- OTHER //-->
			<item><ruleref uri="#commandTurnOnOff" /></item>
			<item><ruleref uri="#commandTurnOnOff" /></item>
		</one-of>
	</rule>
	
	<!--
	
	Object delivery
		Bring / give / take
	
	//-->
	<rule id="commandBring">
		<ruleref uri="#verbBring" />
		<one-of>
		<!-- bring [to whom] [what] //-->
			<item><ruleref uri="#bringReceiver" /><ruleref uri="#bringDirectObject" /></item>
			<!-- bring [to whom] [what] [from where] //-->
			<item><ruleref uri="#bringReceiver" /><ruleref uri="#bringDirectObject" /><ruleref uri="#bringObjectSource" /></item>
			<!-- bring [what] [from where] [to where] //-->
			<item><ruleref uri="#bringDirectObject" /><ruleref uri="#bringObjectSource" /><ruleref uri="#bringDestination" /></item>
			<!-- bring [what] [to where] //-->
			<item><ruleref uri="#bringDirectObject" /><ruleref uri="#bringDestination" /></item>
		</one-of>
	</rule>
	
	<rule id="bringReceiver">
		<one-of>
			<!-- <item><ruleref uri="#adverbPlace" /></item> //-->
			<item><ruleref uri="#pronounPersonal2" /></item>
			<item><ruleref uri="#nounPeople" /></item>
		</one-of>
	</rule>
	
	<rule id="bringDestination">
		<one-of>
			<item><ruleref uri="#adverbPlace" /></item>
			<item><ruleref uri="#prepositionPlace" /><ruleref uri="#place" /></item>
		</one-of>
	</rule>
	
	<rule id="bringObjectSource">
		<item repeat="0-1">that is</item>
		<ruleref uri="#prepositionPlace" />
		<ruleref uri="#place" />
	</rule>
	
	<rule id="bringDirectObject">
		<ruleref uri="#object" />
		<item repeat="0-2">and <ruleref uri="#object" /></item>
	</rule>
	
	<rule id="verbBring">
		<item repeat="0-1"><ruleref uri="#adverb" /></item>
		<one-of>
			<item>bring</item>
			<item>deliver</item>
			<item>give</item>
			<item>move</item>
			<item>take</item>
			<item>get</item>
		</one-of>
		<item repeat="0-1"><ruleref uri="#adverb" /></item>
	</rule>
	
	<!--
	
	Object placing
		Place / give / take
	
	//-->
	<rule id="commandPlace">
		<ruleref uri="#verbPlace" />
		<one-of>
			<!-- place [what] [where] //-->
			<item><ruleref uri="#placeDirectObject" /><ruleref uri="#placeDestination" /></item>
			<!-- give [what] [who] //-->
			<item><ruleref uri="#placeDirectObject" /><ruleref uri="#placeReceiver" /></item>
		</one-of>
	</rule>
	
	<rule id="placeReceiver">
		<ruleref uri="#prepositionPlace" />
		<one-of>
			<item><ruleref uri="#pronounPersonal2" /></item>
			<item><ruleref uri="#nounPeople" /></item>
		</one-of>
	</rule>
	
	<rule id="placeDestination">
		<one-of>
			<item><ruleref uri="#adverbPlace" /></item>
			<item><ruleref uri="#prepositionPlace" /><ruleref uri="#place" /></item>
		</one-of>
	</rule>
	
	<rule id="placeDirectObject">
		<ruleref uri="#object" />
		<item repeat="0-2">and <ruleref uri="#object" /></item>
	</rule>
	
	<rule id="verbPlace">
		<item repeat="0-1"><ruleref uri="#adverb" /></item>
		<one-of>
			<item>bring</item>
			<item>deliver</item>
			<item>carry</item>
			<item>hang out</item>
			<item>give</item>
			<item>place</item>
			<item>put</item>
			<item>put down</item>
			<item>take</item>
			<item>release</item>
		</one-of>
		<item repeat="0-1"><ruleref uri="#adverb" /></item>
	</rule>
	
	<!--
	
	Object delivery
		Take / give / grasp
	
	//-->
	<rule id="commandTake">
		<ruleref uri="#verbTake" />
		<one-of>
			<item><ruleref uri="#takeDirectObject" /></item>
			<!-- take [what] [from who/where] //-->
			<item><ruleref uri="#takeDirectObject" /><ruleref uri="#takeSource" /></item>
			<!-- take [from who/where] [what] //-->
			<item><ruleref uri="#takeSource" /><ruleref uri="#takeDirectObject" /></item>
		</one-of>
	</rule>
	
	<rule id="takeSource">
		<ruleref uri="#prepositionPlace" />
		<one-of>
			<item><ruleref uri="#place" /></item>
			<item><ruleref uri="#people" /></item>
		</one-of>
	</rule>
	
	<rule id="takeDirectObject">
		<ruleref uri="#object" />
		<item repeat="0-2">and <ruleref uri="#object" /></item>
	</rule>
	
	<rule id="verbTake">
		<item repeat="0-1"><ruleref uri="#adverb" /></item>
		<one-of>
			<item>bring</item>
			<item>take</item>
			<item>grasp</item>
			<item>pick up</item>
		</one-of>
		<item repeat="0-1"><ruleref uri="#adverb" /></item>
	</rule>
	
	<!--
	
	Object/people search
		Search / look for / find
	
	//-->
	<rule id="commandSearch">
		<ruleref uri="#verbSearch" />
		<one-of>
			<!-- search [what] //-->
			<item><ruleref uri="#searchDirectObject" /></item>
			<!-- search [what] [where] //-->
			<item><ruleref uri="#searchDirectObject" /><ruleref uri="#searchPlace" /></item>
			<!-- search [where] for [what] //-->
			<item><ruleref uri="#searchPlace" /> for <ruleref uri="#searchDirectObject" /></item>
			<!-- search [what] for [who] //-->
			<item><ruleref uri="#bringDirectObject" /> for <ruleref uri="#people" /></item>
			<!-- search [what] [where] for [who] //-->
			<item><ruleref uri="#searchDirectObject" /><ruleref uri="#searchPlace" /> for <ruleref uri="#people" /></item>
		</one-of>
	</rule>
	
	<rule id="searchPlace">
		<one-of>
			<item><ruleref uri="#adverbPlace" /></item>
			<item><ruleref uri="#prepositionPlace" /><ruleref uri="#place" /></item>
		</one-of>
	</rule>
	
	<rule id="searchDirectObject">
		<one-of>
			<item><ruleref uri="#phraseNounModifier" /><ruleref uri="#noun" /></item>
			<item><ruleref uri="#object" /></item>
		</one-of>
	</rule>
	
	<rule id="verbSearch">
		<item repeat="0-1"><ruleref uri="#adverb" /></item>
		<one-of>
			<item>find</item>
			<item>look</item>
			<item>search</item>
		</one-of>
		<item repeat="0-1"><ruleref uri="#adverb" /></item>
	</rule>
	
	<!--
	
	Follow people
		follow
	
	//-->
	<rule id="commandFollow">
		<ruleref uri="#verbFollow" />
		<one-of>
			<!-- follow [who] [where] //-->
			<item><ruleref uri="#followActor" /><item repeat="0-1"><ruleref uri="#adverb" /></item></item>
			<!-- follow [who] [where] //-->
			<item><ruleref uri="#followActor" /><ruleref uri="#followDestination" /></item>
		</one-of>
	</rule>
	
	<rule id="followActor">
		<one-of>
			<item><ruleref uri="#pronounPersonal2" /></item>
			<item><ruleref uri="#nounPeople" /></item>
		</one-of>
	</rule>
	
	<rule id="followDestination">
		<one-of>
			<item><item repeat="0-1"><ruleref uri="#prepositionPlace" /></item><ruleref uri="#prepositionPlace" /></item>
			<item><item repeat="0-1"><ruleref uri="#prepositionPlace" /></item>there</item>
			<item><ruleref uri="#prepositionPlace" /><ruleref uri="#place" /></item>
		</one-of>
	</rule>
	
	<rule id="followOrigin">
		<ruleref uri="#prepositionPlace" />
		<item repeat="0-1"><ruleref uri="#phraseNounModifier" /></item>
		<ruleref uri="#nounPlace" />
	</rule>
	
	<rule id="verbFollow">
		<item repeat="0-1"><ruleref uri="#adverb" /></item>
		<one-of>
			<item>follow</item>
			<item>come to</item>
			<item>come with</item>
		</one-of>
	</rule>
	
	<!--
	
	Navigate
		go/come/navigate/drive
	
	//-->	
	<rule id="commandNavigate">
		<ruleref uri="#verbNavigate" />
		<one-of>
			<!-- go [where] //-->
			<item><ruleref uri="#navigatePlace" /><item repeat="0-1"><ruleref uri="#adverb" /></item></item>
			<!--go with [who] //-->
			<item><ruleref uri="#navigateActor" /><item repeat="0-1"><ruleref uri="#adverb" /></item></item>
		</one-of>
	</rule>
	
	<rule id="navigateActor">
		<item repeat="0-1"><ruleref uri="#prepositionPlace" /></item>
		<one-of>
			<item>with <ruleref uri="#nounPeople" /></item>
			<item><ruleref uri="#people" /></item>
		</one-of>
	</rule>
	
	<rule id="navigatePlace">
		<item repeat="0-1"><ruleref uri="#prepositionPlace" /></item>
		<one-of>
			<item><ruleref uri="#adverbPlace" /></item>
			<item><ruleref uri="#place" /></item>
		</one-of>
	</rule>
	
	<rule id="verbNavigate">
		<item repeat="0-1"><ruleref uri="#adverb" /></item>
		<one-of>
			<item>go</item>
			<item>come</item>
			<item>move</item>
			<item>navigate</item>
			<item>drive</item>
		</one-of>
	</rule>
	
	<!--
	
	Turn on/off machines
		
	
	//-->
	
	<rule id="commandTurnOnOff">
		
		<one-of>
			<item><ruleref uri="#verbTurnOnOffS" /><ruleref uri="#turnOnOffDirectObject" /></item>
			<item><ruleref uri="#verbTurnOnOffC" /> on <ruleref uri="#turnOnOffDirectObject" /></item>
			<item><ruleref uri="#verbTurnOnOffC" /> off <ruleref uri="#turnOnOffDirectObject" /></item>
			<item><ruleref uri="#verbTurnOnOffC" /><ruleref uri="#turnOnOffDirectObject" /> on</item>
			<item><ruleref uri="#verbTurnOnOffC" /><ruleref uri="#turnOnOffDirectObject" /> off</item>
			<item>shut off<ruleref uri="#turnOnOffDirectObject" /></item>
			<item>shut<ruleref uri="#turnOnOffDirectObject" /> off</item>
		</one-of>
		
	</rule>
	
	<rule id="turnOnOffDirectObject">
		<ruleref uri="#phraseNounModifier" />
		<ruleref uri="#nounMachines" />
		<item repeat="0-1"><ruleref uri="#prepositionPlace" /><ruleref uri="#place" /></item>
	</rule>
	
	<rule id="verbTurnOnOffS">
		<item repeat="0-1"><ruleref uri="#adverb" /></item>
		<one-of>
			<item>start</item>
			<item>stop</item>
			<item>connect</item>
			<item>restart</item>
			<item>reboot</item>
			<item>disconnect</item>
			<item>shutdown</item>
		</one-of>
		<item repeat="0-1"><ruleref uri="#adverb" /></item>
	</rule>
	
	<rule id="verbTurnOnOffC">
		<item repeat="0-1"><ruleref uri="#adverb" /></item>
		<one-of>
			<item>turn</item>
			<item>switch</item>
		</one-of>
	</rule>
	
	<!--
	
	Command extraction from parts
	
	//-->
	
	<rule id="stripped">
		<ruleref uri="#garbage" />
		<one-of>
			<item><ruleref uri="#strippedBring" /></item>
			<item><ruleref uri="#strippedFollow" /></item>
			<item><ruleref uri="#strippedNavigate" /></item>
			<item><ruleref uri="#strippedPlace" /></item>
			<item><ruleref uri="#strippedTake" /></item>
			<item><ruleref uri="#strippedSearch" /></item>
		</one-of>
		<ruleref uri="#garbage" />
	</rule>
	
	<rule id="strippedBring">
		<one-of>
			<!-- Bring me object place //-->
			<item><ruleref uri="#verbBring" /><ruleref uri="#garbage" />me<ruleref uri="#garbage" /><ruleref uri="#nounObject" /><ruleref uri="#garbage" /><ruleref uri="#nounPlace" /></item>
			<!-- Bring object place place //-->
			<item><ruleref uri="#verbBring" /><ruleref uri="#garbage" /><ruleref uri="#nounObject" /><ruleref uri="#garbage" /><ruleref uri="#nounPlace" /><ruleref uri="#garbage" /><ruleref uri="#nounPlace" /></item>
		</one-of>
	</rule>
	
	<rule id="strippedFollow">
		<one-of>
			<!-- Follow person (pronoun) //-->
			<item><ruleref uri="#verbFollow" /><ruleref uri="#garbage" /><ruleref uri="#pronounPersonal2" /></item>
			<!-- Follow person //-->
			<item><ruleref uri="#verbFollow" /><ruleref uri="#garbage" /><ruleref uri="#people" /></item>
			<!-- Follow person place //-->
			<item><ruleref uri="#verbFollow" /><ruleref uri="#garbage" /><ruleref uri="#people" /><ruleref uri="#garbage" /><ruleref uri="#nounPlace" /></item>
		</one-of>
	</rule>
	
	<rule id="strippedNavigate">
		<one-of>
			<!-- Go place //-->
			<item><ruleref uri="#verbNavigate" /><ruleref uri="#garbage" /><ruleref uri="#nounPlace" /></item>
		</one-of>
	</rule>
	
	<rule id="strippedPlace">
		<one-of>
			<!-- Put object place //-->
			<item><ruleref uri="#verbPlace" /><ruleref uri="#garbage" /><ruleref uri="#nounObject" /><ruleref uri="#garbage" /><ruleref uri="#nounPlace" /></item>
			<!-- object Put place //-->
			<item><ruleref uri="#nounObject" /><ruleref uri="#garbage" /><ruleref uri="#verbPlace" /><ruleref uri="#garbage" /><ruleref uri="#nounPlace" /></item>
			<!-- place Put object //-->
			<item><ruleref uri="#nounPlace" /><ruleref uri="#garbage" /><ruleref uri="#verbPlace" /><ruleref uri="#garbage" /><ruleref uri="#nounObject" /></item>
		</one-of>
	</rule>
	
	<rule id="strippedTake">
		<one-of>
			<!-- Take object //-->
			<item><ruleref uri="#verbTake" /><ruleref uri="#garbage" /><ruleref uri="#nounObject" /></item>
			<!-- Take object place //-->
			<item><ruleref uri="#verbTake" /><ruleref uri="#garbage" /><ruleref uri="#nounObject" /><ruleref uri="#garbage" /><ruleref uri="#nounPlace" /></item>
		</one-of>
	</rule>
	
	<rule id="strippedSearch">
		<one-of>
			<!-- Find object //-->
			<item><ruleref uri="#verbSearch" /><ruleref uri="#garbage" /><ruleref uri="#nounObject" /></item>
			<item><ruleref uri="#verbSearch" /><ruleref uri="#garbage" /><ruleref uri="#nounPlaceObject" /></item>
			<!-- Find place //-->
			<item><ruleref uri="#verbSearch" /><ruleref uri="#garbage" /><ruleref uri="#nounPlace" /></item>
			<!-- Find people //-->
			<item><ruleref uri="#verbSearch" /><ruleref uri="#garbage" /><ruleref uri="#people" /></item>
			<!-- Find object place //-->
			<item><ruleref uri="#verbSearch" /><ruleref uri="#garbage" /><ruleref uri="#nounObject" /><ruleref uri="#garbage" /><ruleref uri="#nounPlace" /></item>
			<!-- Find people place //-->
			<item><ruleref uri="#verbSearch" /><ruleref uri="#garbage" /><ruleref uri="#people" /><ruleref uri="#garbage" /><ruleref uri="#nounPlace" /></item>
			<!-- object Find  //-->
			<item><ruleref uri="#nounObject" /><ruleref uri="#garbage" /><ruleref uri="#verbSearch" /></item>
		</one-of>
	</rule>
	
	<!--
	
	Polite
	
	//-->
	
	<rule id="polite">
		<item repeat="0-1">
			<item repeat="0-1">robot</item>
			<one-of>
				<item><ruleref uri="#politePre" /></item>
				<item><ruleref uri="#politeIn" /></item>
			</one-of>
		</item>
	</rule>
	
	<rule id="politePre">
		<one-of>
			<item>please</item>
			<item>let</item>
			<item>lets</item>
			<item>lets go</item>
			<item>let us</item>
			<item>can you</item>
			<item>could you</item>
		</one-of>
	</rule>
	
	<rule id="politeIn">
		<one-of>
			<item>can</item>
			<item>could</item>
			<item>would</item>
		</one-of>
		<item>you</item>
		<item repeat="0-1">please</item>
	</rule>
	
	<!--
	
	Object:
		[mod] nounObject nounObject (apposition)
		[mod] nounObject of nounObject
		[mod] nounObject prepositionPlace [mod] noun (nounPeople, nounPlace, or nounObject)
	//-->
	
	<rule id="object">
		<item repeat="0-1"><ruleref uri="#phraseNounModifier" /></item>
		<ruleref uri="#nounObject" />
		<item repeat="0-1">
			<one-of>
				<item><ruleref uri="#nounObject" /></item>
				<item>of <ruleref uri="#nounObject" /></item>
				<item>
					<ruleref uri="#prepositionPlace" />
					<item repeat="0-1"><ruleref uri="#phraseNounModifier" /></item>
					<ruleref uri="#nounPlace" />
				</item>
				<item>
					<ruleref uri="#prepositionPlace" />
					<item repeat="0-1"><ruleref uri="#phraseNounModifier" /></item>
					<ruleref uri="#noun" />
				</item>
			</one-of>
		</item>
	</rule>
	
	<!--
	
	Place:
		[mod] nounPlace
		[mod] nounPlaceObject of [mod] nounPlaceRoom
		[mod] nounPlace prepositionPlace [mod] noun (nounPeople, nounPlace, or nounObject)
	//-->
	<rule id="place">
		<item repeat="0-1"><ruleref uri="#phraseNounModifier" /></item>
		<one-of>
			<item><ruleref uri="#adverbPlace" /></item>
			<item><ruleref uri="#nounPlace" /></item>
			
			<item>
				<ruleref uri="#nounPlaceObject" />
				of 
				<item repeat="0-1"><ruleref uri="#phraseNounModifier" /></item>
				<ruleref uri="#nounPlaceRoom" />
			</item>
			
			<item>
				<ruleref uri="#nounPlace" />
				<ruleref uri="#prepositionPlace" />
				<item repeat="0-1"><ruleref uri="#phraseNounModifier" /></item>
				<ruleref uri="#noun" />
			</item>
		</one-of>
	</rule>
	
	<!--
	
	People:
		[mod] nounPeople
		[mod] nounPeople prepositionPlace [mod] noun (nounPeople, nounPlace, or nounObject)
	//-->
	<rule id="people">
		<item repeat="0-1"><ruleref uri="#phraseNounModifier" /></item>
		<ruleref uri="#nounPeople" />
		<!--
		<item repeat="0-1">
			<ruleref uri="#prepositionPlace" />
			<item repeat="0-1"><ruleref uri="#phraseNounModifier" /></item>
			<ruleref uri="#noun" />
		</item>
		//-->
	</rule>
	
	<!--
	
	Noun phrase:
		A noun phrase has a noun (flight), which usually has a determiner (a) and/or adjective (good) in front of it. A noun phrase can also be a pronoun (we).
		A prepositional phrase or adverb phrase can come after the noun and modify it.
	
	//-->
	
	<rule id="phraseNounModifier">
		<!--
			Determiners, quantifiers and modifiers come in a fixed order before the noun.
			All determiners goes before the modifiers and the noun: determiners + modifiers + noun.
			We can also use a determiner after a quantifier + of (some of my).
			We can also use an article or demonstrative before a quantifier + of (a couple of the).
			A prepositional phrase or adverb phrase can come after the noun and modify it.
		//-->
		<item repeat="0-1">
			<one-of>
				<item><ruleref uri="#determiner" /></item>
				<!--
				<item><ruleref uri="#determinerQuantifier" /><ruleref uri="#determinerNQ" /></item>
				<item><ruleref uri="#determinerDemonstrative" /><ruleref uri="#determinerQuantifier" /><ruleref uri="#determinerNQ" /></item>
				//-->
			</one-of>
		</item>
		<item repeat="0-1"><ruleref uri="#phraseAdjective" /></item>
	</rule>
	
	<!--
	
	Adjective phrase:
		An adjective phrase has an adjective, sometimes with an adverb of degree (very).
	
	//-->
	<rule id="phraseAdjective">
		<item repeat="0-1"><ruleref uri="#adverbDegree" /></item>
		<item><ruleref uri="#adjective" /></item>
	</rule>
	
	<!--
	
	Adverb phrase:
		An adverb phrase has an adverb, sometimes with an adverb of degree (almost).
	
	//-->
	<rule id="phraseAdverb">
		<one-of>
			<item><ruleref uri="#adverb" /></item>
			<item><ruleref uri="#adverbDegree" /><ruleref uri="#adverbNoDegree" /></item>
		</one-of>
	</rule>
			
	<!--
	
	Pronouns:
		Pronouns can be:
			Personal:		I (me), you, he (him), her (she), iy, we (us), and they (them)
			Reflexive:		myself, yourself, himself, herself, itself, ourselves, yourselves, themselves
			Possessive:		my, your, his, her, its, our, their
			Demonstrative:	this, that, these, those
	
	//-->
	
	<rule id="pronounPersonal2">
		<one-of>
			<item>me</item>
			<!--
			<item>her</item>
			<item>his</item>
			//-->
			<item>us</item>
			<item>them</item>
		</one-of>
	</rule>
	
	<rule id="pronounDemonstrative">
		<one-of>
			<item>this</item>
			<item>that</item>
			<!--
			<item>these</item>
			<item>those</item>
			//-->
		</one-of>
	</rule>
	
	<rule id="pronounRelative">
		<one-of>
			<!--
			<item>who</item>
			<item>whom</item>
			//-->
			<item>which</item>
			<item>that</item>
		</one-of>
	</rule>
	
	<!--
	
	Preposition:
		A preposition is a word like in, to, for, out of
		Propositions can be of:
			Place:		in, under, across
			Time:		at, before, for	
	//-->
	<rule id="prepositionPlace">
		<one-of>
			<item>along</item>
			<item>around</item>
			<item>at</item>
			<item>away from</item>
			<item>behind</item>
			<item>between</item>
			<item>close to</item>
